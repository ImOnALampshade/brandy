class vec3
{
  var v : float[3]

  property x:float
    get: v[0]
    set: v[0] = value

  property y:float
    get: v[1]
    set: v[1] = value

  property z:float
    get: v[2]
    set: v[2] = value

  func @create(_x as float = 0, _y as float = 0, _z as float = 0)
  {
    v[0]=_x
    v[1]=_y
    v[2]=_z
  }
}
